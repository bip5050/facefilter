{
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.9.11",
    "browser-sync-webpack-plugin": "^1.0.0",
    "css-loader": "^0.21.0",
    "ctx-get-transform": "^1.0.2",
    "eslint": "^1.7.3",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.1.0",
    "exports-loader": "^0.6.2",
    "gl-matrix": "^2.3.1",
    "glslify-loader": "^1.0.2",
    "jade": "^1.11.0",
    "jade-loader": "^0.7.1",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.3",
    "node-sass": "^3.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "dev": "webpack -d --devtool inline-source-map --progress --colors --watch",
    "release": "webpack -p --progress --colors"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": 2,
      "arrow-parens": 1,
      "arrow-spacing": 1,
      "constructor-super": 2,
      "generator-star-spacing": 2,
      "no-class-assign": 2,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "no-empty-pattern": 2,
      "no-this-before-super": 2,
      "no-var": 2,
      "object-shorthand": 2,
      "prefer-arrow-callback": 2,
      "prefer-const": 0,
      "prefer-reflect": 2,
      "prefer-spread": 2,
      "prefer-template": 2,
      "require-yield": 2,
      "no-console": 1,
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-unreachable": 1,
      "no-debugger": 1
    },
    "env": {
      "es6": true,
      "browser": true,
      "amd": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "modules": true
    }
  }
}
